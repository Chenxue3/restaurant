{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/src/app.js"
    }
  ]
}
